{
    "name": "codelint/qr",
    "type": "project",
    "description": "QR helper",
    "keywords": [
        "qr",
        "laravel",
        "module"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "codelint/qr-code": "3.7.2.2",
        "fideloper/proxy": "^4.0",
        "laravel/framework": ">5.8",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Qr\\": "modules/qr/src/"
        },
        "classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    }
}
