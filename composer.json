{
    "name": "codelint/qr",
    "type": "project",
    "description": "QR helper",
    "keywords": [
        "qr",
        "laravel",
        "module"
    ],
    "license": "MIT",
    "require": {
        "php": ">7.1.3",
        "endroid/qr-code": "3.9.1",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Qr\\": "modules/qr/src/"
        },
        "classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    }
}
